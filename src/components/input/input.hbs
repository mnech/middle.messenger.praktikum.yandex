<div class="{{propStyle}}">
  <label class="{{styles.label}}">
    <span>{{label}}</span>
    <span>{{value}}</span>
    <input class="{{styles.input}}" type={{type}} name={{name}} placeholder="{{placeholder}}" value="{{value}}">
  </label>
  <p class="{{styles.error}}">{{errorText}}</p>
</div>
